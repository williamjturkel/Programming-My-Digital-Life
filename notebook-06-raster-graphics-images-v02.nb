Cell[CellGroupData[{

Cell["The Raster command", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.663179984053647*^9, 3.6631800099220953`*^9}}],

Cell[TextData[{
 "The ",
 StyleBox["Raster",
  FontWeight->"Bold"],
 " command allows us to describe a graphic element in terms of rows and \
columns of white, black, grayscale or coloured ",
 StyleBox["pixels",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.6631800234403257`*^9, 3.663180104359087*^9}, {
  3.663180215538905*^9, 3.6631802276329927`*^9}, {3.6631802827656193`*^9, 
  3.663180287731102*^9}}],

Cell["\<\
Here is how we draw a single black pixel. Note the coordinates.\
\>", "Text",
 CellChangeTimes->{{3.6631801198764687`*^9, 3.6631801525213537`*^9}, {
  3.663180183125469*^9, 3.663180250294345*^9}, {3.695313643359243*^9, 
  3.695313655446755*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"Raster", "[", 
    RowBox[{"{", 
     RowBox[{"{", "0", "}"}], "}"}], "]"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Small"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.663048283771574*^9, 3.663048298914262*^9}, {
   3.663048357084038*^9, 3.663048385847886*^9}, 3.663048450896418*^9, {
   3.6630484826205883`*^9, 3.663048483148017*^9}}],

Cell["Here is a single white pixel.", "Text",
 CellChangeTimes->{{3.663180258645499*^9, 3.663180264780772*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"Raster", "[", 
    RowBox[{"{", 
     RowBox[{"{", "1", "}"}], "}"}], "]"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Small"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.663048283771574*^9, 3.663048298914262*^9}, {
  3.663048357084038*^9, 3.663048385847886*^9}, {3.663048462942998*^9, 
  3.663048486979525*^9}}],

Cell[TextData[{
 "We give the ",
 StyleBox["Raster",
  FontWeight->"Bold"],
 " command a nested list. We can think of this list as containing a list of \
rows, each of which is comprised of a list of columns. The raster image is \
built one row at a time from the lower left hand corner."
}], "Text",
 CellChangeTimes->{{3.663180285375164*^9, 3.663180339916552*^9}}],

Cell["\<\
Here is a raster image containing one row. The row consists of two columns, a \
black pixel followed by a white one.\
\>", "Text",
 CellChangeTimes->{{3.663180346395471*^9, 3.663180392262775*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"Raster", "[", 
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{"0", ",", "1"}], "}"}], "}"}], "]"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Small"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.663048283771574*^9, 3.663048298914262*^9}, {
   3.663048357084038*^9, 3.663048385847886*^9}, 3.663048450896418*^9, {
   3.6630484826205883`*^9, 3.663048483148017*^9}, {3.663048616803988*^9, 
   3.663048616939703*^9}}],

Cell["\<\
This raster image is also one row high by two columns wide. The first pixel \
is white this time.\
\>", "Text",
 CellChangeTimes->{{3.6631804000129833`*^9, 3.6631804169716682`*^9}, {
  3.663180567052024*^9, 3.6631805710356607`*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"Raster", "[", 
    RowBox[{"{", 
     RowBox[{"{", 
      RowBox[{"1", ",", "0"}], "}"}], "}"}], "]"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"ImageSize", "\[Rule]", "Small"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.663048283771574*^9, 3.663048298914262*^9}, {
   3.663048357084038*^9, 3.663048385847886*^9}, 3.663048450896418*^9, {
   3.6630484826205883`*^9, 3.663048483148017*^9}, {3.663048616803988*^9, 
   3.663048626825893*^9}}],

