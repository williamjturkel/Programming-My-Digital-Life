Cell[TextData[{
 "We can use ",
 StyleBox["RGBColor",
  FontWeight->"Bold"],
 " to see a paint chip"
}], "Text",
 CellChangeTimes->{{3.664347613940013*^9, 3.6643476282032423`*^9}}],

