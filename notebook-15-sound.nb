Cell[CellGroupData[{

Cell["Representing and playing notes", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.666084720463738*^9, 3.6660847225752373`*^9}, {3.6660851237448606`*^9, 
  3.666085127007636*^9}, {3.6660851842063437`*^9, 3.6660852044926853`*^9}}],

Cell[TextData[{
 "In ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 ", you can refer to a named note using the ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " command with a string argument. This is how you write Middle C. Note that \
the ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " command doesn\[CloseCurlyQuote]t make a sound by itself."
}], "Text",
 CellChangeTimes->{{3.666084724959126*^9, 3.666084735510713*^9}, {
  3.666085136160419*^9, 3.666085151598693*^9}, {3.6660852075803967`*^9, 
  3.666085271162098*^9}}],

Cell[BoxData[
 RowBox[{"SoundNote", "[", "\"\<C\>\"", "]"}]], "Input",
 CellChangeTimes->{{3.6660851899884243`*^9, 3.6660851945179853`*^9}}],

Cell[TextData[{
 "If you want to give the user an interface for playing notes, use ",
 StyleBox["Sound",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.666085276969801*^9, 3.666085297129033*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"SoundNote", "[", "\"\<C\>\"", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.666085303592222*^9, 3.666085308984672*^9}}],

Cell[TextData[{
 "A sequence of notes is represented by a list of ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " commands."
}], "Text",
 CellChangeTimes->{{3.6660853264004717`*^9, 3.66608536781435*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"SoundNote", "[", "\"\<C\>\"", "]"}], ",", 
    RowBox[{"SoundNote", "[", "\"\<C\>\"", "]"}], ",", 
    RowBox[{"SoundNote", "[", "\"\<G\>\"", "]"}]}], "}"}], "]"}]], "Input",
 CellChangeTimes->{{3.666085303592222*^9, 3.666085308984672*^9}, {
  3.666085379950738*^9, 3.666085393157467*^9}}],

Cell[CellGroupData[{

Cell["Chords", "Subsection",
 CellChangeTimes->{{3.666085620732626*^9, 3.666085621292664*^9}}],

Cell[TextData[{
 "A chord (two or more notes played at the same time) is represented by \
giving ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " a list"
}], "Text",
 CellChangeTimes->{{3.666085413085189*^9, 3.666085460074532*^9}, {
  3.6660854930814734`*^9, 3.666085494241474*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"{", 
   RowBox[{"SoundNote", "[", 
    RowBox[{"{", 
     RowBox[{"\"\<C\>\"", ",", "\"\<G\>\""}], "}"}], "]"}], "}"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.666085303592222*^9, 3.666085308984672*^9}, {
  3.666085379950738*^9, 3.666085393157467*^9}, {3.6660854819309883`*^9, 
  3.666085490033717*^9}}],

Cell["You can specify absolute times, with an overlap, like this", "Text",
 CellChangeTimes->{{3.666087856170772*^9, 3.666087883961308*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"SoundNote", "[", 
     RowBox[{"\"\<C\>\"", ",", 
      RowBox[{"{", 
       RowBox[{"0", ",", "0.3"}], "}"}]}], "]"}], ",", 
    RowBox[{"SoundNote", "[", 
     RowBox[{"\"\<G\>\"", ",", 
      RowBox[{"{", 
       RowBox[{"0.1", ",", "0.5"}], "}"}]}], "]"}]}], "}"}], "]"}]], "Input",
 CellID->520380054],

Cell["A chord of C, E, G and Bb", "Text",
 CellChangeTimes->{{3.666087958159232*^9, 3.66608796441406*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"SoundNote", "[", 
   RowBox[{"{", 
    RowBox[{
    "\"\<C\>\"", ",", "\"\<E\>\"", ",", "\"\<G\>\"", ",", "\"\<Bb\>\""}], 
    "}"}], "]"}], "]"}]], "Input",
 CellID->651703356]
}, Open  ]],

Cell[CellGroupData[{

Cell["Duration", "Subsection",
 CellChangeTimes->{{3.666085624285323*^9, 3.666085625068343*^9}}],

Cell[TextData[{
 "By default, a note is played for 1 second. You can give ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " a second argument which is the duration of a note in seconds. Here is how \
you play Middle C for 1/5th of a second."
}], "Text",
 CellChangeTimes->{{3.666085512505396*^9, 3.666085601797373*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"SoundNote", "[", 
   RowBox[{"\"\<C\>\"", ",", ".2"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.666085303592222*^9, 3.666085308984672*^9}, {
  3.666085583414007*^9, 3.666085586381999*^9}}],

Cell[TextData[{
 "You can specify a ",
 StyleBox["rest",
  FontSlant->"Italic"],
 " with ",
 StyleBox["None",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.6660878924487658`*^9, 3.666087898112484*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"SoundNote", "[", 
     RowBox[{"\"\<C\>\"", ",", "0.2"}], "]"}], ",", 
    RowBox[{"SoundNote", "[", 
     RowBox[{"None", ",", "0.2"}], "]"}], ",", 
    RowBox[{"SoundNote", "[", 
     RowBox[{"\"\<G\>\"", ",", "0.3"}], "]"}]}], "}"}], "]"}]], "Input",
 CellID->44056557]
}, Open  ]],

Cell[CellGroupData[{

Cell["Instrument", "Subsection",
 CellChangeTimes->{{3.666085659547903*^9, 3.666085661971086*^9}}],

Cell[TextData[{
 "The default instrument is a piano. You can give ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " a third argument, a string, to choose another instrument."
}], "Text",
 CellChangeTimes->{{3.6660856730832663`*^9, 3.666085684826188*^9}, {
  3.666085721673627*^9, 3.66608573273625*^9}}],

Cell[BoxData[
 RowBox[{"Sound", "[", 
  RowBox[{"SoundNote", "[", 
   RowBox[{"\"\<C\>\"", ",", "1.5", ",", "\"\<Tuba\>\""}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.666085303592222*^9, 3.666085308984672*^9}, {
  3.666085583414007*^9, 3.666085586381999*^9}, {3.6660856970337152`*^9, 
  3.666085703009576*^9}}],

Cell[TextData[{
 "The help file for ",
 StyleBox["SoundNote",
  FontWeight->"Bold"],
 " has a complete listing of instruments."
}], "Text",
 CellChangeTimes->{{3.66608688351058*^9, 3.666086893245734*^9}}]
}, Open  ]]
}, Open  ]]
}, Open  ]],

