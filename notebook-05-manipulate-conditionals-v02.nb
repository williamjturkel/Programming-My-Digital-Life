Cell[CellGroupData[{

Cell["Scanned by a UFO", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.662792005873192*^9, 3.662792008039876*^9}}],

Cell["\<\
Suppose we want our UFO to randomly \[OpenCurlyQuote]scan\[CloseCurlyQuote] \
the plane. We start by making a cone-shaped beam of partially transparent \
light. We use an elliptical disk sector to round the bottom of the cone.\
\>", "Text",
 CellChangeTimes->{{3.66220478048505*^9, 3.662204913743929*^9}, {
  3.662205288407145*^9, 3.662205360453714*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawBeam", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Opacity", "[", 
     RowBox[{"1", "/", "2"}], "]"}], ",", "Yellow", ",", 
    RowBox[{"Polygon", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "-", "2"}], ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "2"}], ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "10"}], ",", 
         RowBox[{"y", "-", "18"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "-", "10"}], ",", 
         RowBox[{"y", "-", "18"}]}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"y", "-", "18"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"10", ",", 
        RowBox[{"3", "/", "2"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"180", "Degree"}], ",", 
        RowBox[{"360", " ", "Degree"}]}], "}"}]}], "]"}]}], "}"}]}]], "Input",\

 CellChangeTimes->{{3.662204942512204*^9, 3.662205175816661*^9}, {
   3.6622052382269697`*^9, 3.662205246951551*^9}, 3.6622052823369217`*^9, {
   3.6622053709483557`*^9, 3.6622054127542257`*^9}, {3.6622055809347267`*^9, 
   3.662205604263783*^9}, {3.662205750072967*^9, 3.662205755326906*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"drawBeam", "[", 
    RowBox[{"{", 
     RowBox[{"0", ",", "0"}], "}"}], "]"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.66220501305643*^9, 3.662205030248514*^9}}],

Cell[TextData[{
 "Then we create another drawing function that usually just draws the UFO, \
but sometimes draws the beam beneath it. We will give this function an \
argument ",
 StyleBox["p",
  FontSlant->"Italic"],
 " that allows us to control the probability of scanning."
}], "Text",
 CellChangeTimes->{{3.66220478048505*^9, 3.662204913743929*^9}, {
  3.662205288407145*^9, 3.66220529234904*^9}, {3.662205422720743*^9, 
  3.6622054235197687`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawScanningUFO", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"x_", ",", "y_"}], "}"}], ",", "p_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"RandomChoice", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"p", ",", 
        RowBox[{"1", "-", "p"}]}], "}"}], "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"drawBeam", "[", 
         RowBox[{"{", 
          RowBox[{"x", ",", "y"}], "}"}], "]"}], ",", 
        RowBox[{"Point", "[", 
         RowBox[{"{", 
          RowBox[{"x", ",", "y"}], "}"}], "]"}]}], "}"}]}], "]"}], ",", 
    RowBox[{"drawUFO", "[", 
     RowBox[{"{", 
      RowBox[{"x", ",", "y"}], "}"}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.66220485227747*^9, 3.662204931220141*^9}, {
  3.66220545301761*^9, 3.662205500694787*^9}}],

Cell["\<\
By setting the probability to 1, we can see what it looks like with the beam.\
\
\>", "Text",
 CellChangeTimes->{{3.662205529791857*^9, 3.6622055445325613`*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"drawScanningUFO", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"0", ",", "0"}], "}"}], ",", "1"}], "]"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662205515296076*^9, 3.6622055536231203`*^9}}],

Cell["\<\
Here is the new animation, with the scanning probability set to 65%\
\>", "Text",
 CellChangeTimes->{{3.662205624843638*^9, 3.6622056256669807`*^9}, {
  3.6622056792082767`*^9, 3.662205698107478*^9}, {3.662205812314973*^9, 
  3.6622058131460667`*^9}}],

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"background", ",", 
      RowBox[{"drawPlane", "[", 
       RowBox[{"{", 
        RowBox[{"x", ",", "75"}], "}"}], "]"}], ",", 
      RowBox[{"drawScanningUFO", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "-", 
           RowBox[{"RandomInteger", "[", "2", "]"}]}], ",", 
          RowBox[{"90", "+", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"RandomInteger", "[", "2", "]"}], "-", "1"}], ")"}]}]}], 
         "}"}], ",", ".65"}], "]"}]}], "}"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "12", ",", "88"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662202541873095*^9, 3.662202572535338*^9}, {
   3.662202787912957*^9, 3.66220278806933*^9}, 3.662203002226962*^9, {
   3.6622030669044523`*^9, 3.662203070679249*^9}, {3.6622031042634287`*^9, 
   3.662203106823895*^9}, {3.662203201659875*^9, 3.662203204073851*^9}, {
