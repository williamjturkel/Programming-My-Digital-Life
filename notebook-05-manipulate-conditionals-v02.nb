       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", ".5"}], ",", 
         RowBox[{"y", "+", ".5"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", ".8"}], ",", 
         RowBox[{"y", "+", ".8"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "1.5"}], ",", 
         RowBox[{"y", "+", "1"}]}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"1.2", ",", ".8"}], "}"}]}], "]"}], ",", 
    RowBox[{"If", "[", 
     RowBox[{"lit", ",", 
      RowBox[{"Lighter", "[", "Green", "]"}], ",", 
