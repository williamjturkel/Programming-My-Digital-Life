Cell[CellGroupData[{

Cell["Simple animation with Manipulate", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.662203310021667*^9, 3.662203315091049*^9}}],

Cell[CellGroupData[{

Cell["A routine flight", "Subsection",
 CellChangeTimes->{{3.662205636768157*^9, 3.662205649093918*^9}}],

Cell[TextData[{
 "One of the things we can do with ",
 StyleBox["Manipulate",
  FontWeight->"Bold"],
 " is use it to create simple animations."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.662203328136526*^9, 3.6622033540256453`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawPlane", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Darker", "[", "Gray", "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"10", ",", "1"}], "}"}]}], "]"}], ",", 
    RowBox[{"Rotate", "[", 
     RowBox[{
      RowBox[{"Disk", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "-", "9"}], ",", 
          RowBox[{"y", "+", 
           RowBox[{"3", "/", "2"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", 
          RowBox[{"1", "/", "2"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"100", " ", "Degree"}]}], "]"}], ",", 
    RowBox[{"Rotate", "[", 
     RowBox[{
      RowBox[{"Disk", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "y"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"5", ",", "1"}], "}"}]}], "]"}], ",", 
      RowBox[{"135", "Degree"}]}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.662203376893289*^9, 3.662203756892147*^9}, {
  3.662203874186697*^9, 3.662203877720179*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"drawPlane", "[", 
    RowBox[{"{", 
     RowBox[{"0", ",", "0"}], "}"}], "]"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662203392505734*^9, 3.662203414286953*^9}}],

Cell["Here our plane flies smoothly across the sky.", "Text",
 CellChangeTimes->{{3.662203939291155*^9, 3.66220394770525*^9}, {
  3.662204162563222*^9, 3.6622042041211777`*^9}}],

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"background", ",", 
      RowBox[{"drawPlane", "[", 
       RowBox[{"{", 
        RowBox[{"x", ",", "75"}], "}"}], "]"}]}], "}"}], "]"}], ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "12", ",", "88"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662202541873095*^9, 3.662202572535338*^9}, {
   3.662202787912957*^9, 3.66220278806933*^9}, 3.662203002226962*^9, {
   3.6622030669044523`*^9, 3.662203070679249*^9}, {3.6622031042634287`*^9, 
   3.662203106823895*^9}, {3.662203201659875*^9, 3.662203204073851*^9}, {
   3.662203234795876*^9, 3.662203235219643*^9}, {3.662203804265028*^9, 
   3.662203863875437*^9}, {3.6622038976207027`*^9, 3.6622038986680517`*^9}, {
   3.662204150670059*^9, 3.662204159067465*^9}, {3.6622042071209373`*^9, 
   3.662204209256617*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Close encounter", "Subsection",
 CellChangeTimes->{{3.662205655117113*^9, 3.662205658196136*^9}}],

Cell["\<\
Now suppose we add a UFO. If we want the UFO to move with the plane, we can \
use the same variable to control each.\
\>", "Text",
 CellChangeTimes->{{3.662203951567683*^9, 3.6622039536718197`*^9}, {
  3.662204023160451*^9, 3.66220402933537*^9}, {3.662204083188205*^9, 
  3.6622041204916563`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawUFO", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{"Orange", ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"6", ",", "2"}], "}"}]}], "]"}], ",", "LightOrange", ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"y", "+", "1"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"3", ",", "2"}], "}"}]}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.658345349365903*^9, 3.658345443625517*^9}, {
  3.658345608050774*^9, 3.658345612121172*^9}, {3.658345715190041*^9, 
  3.658345746426568*^9}, {3.658346349678823*^9, 3.658346351813287*^9}, {
  3.662204273107482*^9, 3.662204292030471*^9}}],

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"background", ",", 
      RowBox[{"drawPlane", "[", 
       RowBox[{"{", 
        RowBox[{"x", ",", "75"}], "}"}], "]"}], ",", 
      RowBox[{"drawUFO", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "-", "4"}], ",", "90"}], "}"}], "]"}]}], "}"}], "]"}], 
   ",", 
   RowBox[{"{", 
    RowBox[{"x", ",", "12", ",", "88"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.662202541873095*^9, 3.662202572535338*^9}, {
   3.662202787912957*^9, 3.66220278806933*^9}, 3.662203002226962*^9, {
   3.6622030669044523`*^9, 3.662203070679249*^9}, {3.6622031042634287`*^9, 
   3.662203106823895*^9}, {3.662203201659875*^9, 3.662203204073851*^9}, {
   3.662203234795876*^9, 3.662203235219643*^9}, {3.662203804265028*^9, 
   3.662203863875437*^9}, {3.6622038976207027`*^9, 3.6622038986680517`*^9}, {
   3.662204221157927*^9, 3.6622043092110577`*^9}}],

Cell[TextData[{
 "If we add some noise to the ",
 StyleBox["x",
  FontSlant->"Italic"],
 " and ",
 StyleBox["y",
  FontSlant->"Italic"],
 " positions of the UFO, we can get it to fly more erratically."
}], "Text",
 CellChangeTimes->{{3.662204331326275*^9, 3.6622043602397346`*^9}, {
  3.6622044182677813`*^9, 3.662204431096023*^9}, {3.662204575874277*^9, 
  3.662204585030755*^9}}],

Cell[BoxData[
 RowBox[{"Manipulate", "[", 
  RowBox[{
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
     RowBox[{"background", ",", 
      RowBox[{"drawPlane", "[", 
       RowBox[{"{", 
        RowBox[{"x", ",", "75"}], "}"}], "]"}], ",", 
      RowBox[{"drawUFO", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "-", 
          RowBox[{"RandomInteger", "[", "2", "]"}]}], ",", 
         RowBox[{"90", "+", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"RandomInteger", "[", "2", "]"}], "-", "1"}], ")"}]}]}], 
        "}"}], "]"}]}], "}"}], "]"}], ",", 
   RowBox[{"{", 
