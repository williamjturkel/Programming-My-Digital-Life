}], "Text",
 CellChangeTimes->{{3.664477855845604*^9, 3.664477882069355*^9}}],

Cell[BoxData[
 RowBox[{"ImageCollage", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"applesThumb", ",", "gSmithThumb", ",", 
     RowBox[{"Graphics", "[", "labelledApple", "]"}], ",", 
     RowBox[{"ImageRotate", "[", 
