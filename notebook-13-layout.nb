      RowBox[{"RandomInteger", "[", "20", "]"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"r", ",", "6"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"c", ",", "10"}], "}"}]}], "]"}], "]"}]], "Input",
