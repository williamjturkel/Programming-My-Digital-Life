Cell["Partitioning and Assembling Images", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.664469056643793*^9, 3.664469062146854*^9}}],

Cell[TextData[{
 "The ",
 StyleBox["ImagePartitition",
  FontWeight->"Bold"],
 " command divides an image into an array of subimages"
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.6644675227521477`*^9, 3.66446753583072*^9}, {3.664467627270482*^9, 
  3.664467641494397*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"applesThumbPartition", "=", 
   RowBox[{"ImagePartition", "[", 
    RowBox[{"applesThumb", ",", "15"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.664467756958976*^9, 3.664467769798979*^9}, {
  3.6644681013108387`*^9, 3.664468101693454*^9}}],

Cell[BoxData[
 RowBox[{"Grid", "[", "applesThumbPartition", "]"}]], "Input",
 CellChangeTimes->{{3.664467538102992*^9, 3.66446755700679*^9}, {
  3.664467599103507*^9, 3.664467620351268*^9}, {3.664467767506029*^9, 
  3.6644677770066*^9}}],

Cell[TextData[{
 "We can pull out individual subimages with ",
 StyleBox["Part",
  FontWeight->"Bold"]
}], "Text",
 CellChangeTimes->{{3.6644677889105873`*^9, 3.664467803446186*^9}}],

Cell[BoxData[
 RowBox[{"applesThumbPartition", "\[LeftDoubleBracket]", 
  RowBox[{"1", ",", "1"}], "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.664467809950777*^9, 3.66446781558208*^9}}],

Cell[BoxData[
 RowBox[{"applesThumbPartition", "\[LeftDoubleBracket]", "1", 
  "\[RightDoubleBracket]"}]], "Input",
 CellChangeTimes->{{3.664467809950777*^9, 3.66446781558208*^9}, 
   3.66446787083878*^9}],

Cell[TextData[{
 StyleBox["ImageAssemble",
  FontWeight->"Bold"],
 " puts the pieces back together"
}], "Text",
 CellChangeTimes->{{3.6644676520784683`*^9, 3.664467657526491*^9}, {
  3.664467733039097*^9, 3.664467736694153*^9}}],

Cell[BoxData[
 RowBox[{"ImageAssemble", "[", "applesThumbPartition", "]"}]], "Input",
 CellChangeTimes->{{3.664467538102992*^9, 3.66446755700679*^9}, {
  3.664467599103507*^9, 3.664467620351268*^9}, {3.6644676667991943`*^9, 
  3.664467668886504*^9}, {3.664467912055068*^9, 3.664467913661553*^9}, {
  3.664469027116598*^9, 3.664469029947063*^9}}],

Cell["\<\
Of course, we can modify the list of subimages before we join them back \
together \
\>", "Text",
 CellChangeTimes->{{3.664468217773643*^9, 3.6644682206848307`*^9}, {
  3.664468482525422*^9, 3.6644685253801928`*^9}}],

Cell[BoxData[
 RowBox[{"ImageAssemble", "[", 
  RowBox[{"Join", "[", 
   RowBox[{
    RowBox[{"applesThumbPartition", "\[LeftDoubleBracket]", 
     RowBox[{"1", ";;", "2"}], "\[RightDoubleBracket]"}], ",", 
    RowBox[{"{", 
     RowBox[{"Reverse", "[", 
      RowBox[{
      "applesThumbPartition", "\[LeftDoubleBracket]", "3", 
       "\[RightDoubleBracket]"}], "]"}], "}"}], ",", 
    RowBox[{"applesThumbPartition", "\[LeftDoubleBracket]", 
     RowBox[{"4", ";;", "6"}], "\[RightDoubleBracket]"}]}], "]"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.664467538102992*^9, 3.66446755700679*^9}, {
