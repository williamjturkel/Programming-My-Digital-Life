Cell[CellGroupData[{

Cell["Coordinates and scale", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.658338805037574*^9, 3.658338822227734*^9}, {3.6583388846573143`*^9, 
  3.65833888598451*^9}, {3.658339081470874*^9, 3.658339090830378*^9}}],

Cell[TextData[{
 "Unless you specify otherwise, ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " draws a circle or disk centred at {0,0} with a radius of 1. We can display \
the coordinate system by giving ",
 StyleBox["Graphics",
  FontWeight->"Bold"],
 " the options ",
 StyleBox["Gridlines \[Rule] Automatic",
  FontSlant->"Italic"],
 " and ",
 StyleBox["Frame \[Rule] True",
  FontSlant->"Italic"],
 "."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.658339028170498*^9, 3.658339141203908*^9}, {3.6583412214219227`*^9, 
  3.658341221574006*^9}}],

Cell["You make the little arrows by typing \[EscapeKey]->\[EscapeKey]", "Text",
 CellChangeTimes->{{3.658339153011939*^9, 3.6583391773060637`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Thick", ",", 
     RowBox[{"Circle", "[", "]"}]}], "}"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.658337831451344*^9, 3.6583378785702353`*^9}, {
  3.6583389808127947`*^9, 3.658339019723778*^9}, {3.658339057041514*^9, 
  3.6583390614838*^9}}],

Cell[BoxData[
 GraphicsBox[
  {Thickness[Large], CircleBox[{0, 0}]},
  Frame->True,
  GridLines->Automatic]], "Output",
 CellChangeTimes->{{3.658338987374457*^9, 3.6583390203008623`*^9}, {
   3.658339057781148*^9, 3.65833906992428*^9}, 3.660151395851366*^9}]
}, Open  ]],

