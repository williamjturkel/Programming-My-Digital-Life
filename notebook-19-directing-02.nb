       RowBox[{
        RowBox[{"x", "+", "3.5"}], ",", 
        RowBox[{"y", "+", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"RandomInteger", "[", "4", "]"}], "+", "8"}], ")"}]}]}], 
       "}"}], ",", 
      RowBox[{"RoundingRadius", "\[Rule]", ".7"}]}], "]"}], ",", 
    RowBox[{"Rectangle", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "+", "0.55"}], ",", 
        RowBox[{"y", "+", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"RandomInteger", "[", "1", "]"}], "+", "1"}], ")"}]}]}], 
       "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "+", "2.05"}], ",", 
