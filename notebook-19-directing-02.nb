Cell["\<\
There are going to be some mountains in the background. The more distant will \
be lighter in colour (this is called atmospheric perspective).\
\>", "Text",
 CellChangeTimes->{{3.667310444834746*^9, 3.667310471329714*^9}, {
  3.6673107891342154`*^9, 3.667310799733137*^9}, {3.6673108376596212`*^9, 
  3.66731083793987*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawMountains", "=", 
   RowBox[{"{", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"Blend", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"LightGray", ",", 
            RowBox[{"Darker", "[", "Gray", "]"}]}], "}"}], ",", "i"}], "]"}], 
        ",", 
        RowBox[{"Polygon", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "viewFinderHeight"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"RandomInteger", "[", "200", "]"}], "+", "100"}], ",", 
             RowBox[{"viewFinderHeight", "+", 
              RowBox[{"RandomInteger", "[", "100", "]"}], "+", 
              RowBox[{"50", 
               RowBox[{"(", 
                RowBox[{"1", "-", "i"}], ")"}]}]}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"RandomInteger", "[", "300", "]"}], "+", "400"}], ",", 
             RowBox[{"viewFinderHeight", "+", 
              RowBox[{"RandomInteger", "[", "50", "]"}], "+", 
              RowBox[{"100", 
               RowBox[{"(", 
                RowBox[{"1", "-", "i"}], ")"}]}]}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"sceneWidth", ",", "viewFinderHeight"}], "}"}]}], "}"}], 
         "]"}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", 
        RowBox[{"1", "/", "4"}], ",", "1", ",", 
        RowBox[{"1", "/", "4"}]}], "}"}]}], "]"}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.6673109325043716`*^9, 3.667310935831616*^9}, {
  3.667310968506378*^9, 3.667310982905693*^9}, {3.6673110133408747`*^9, 
  3.667311092680339*^9}, {3.667311167214912*^9, 3.667311177518847*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"drawMountains", ",", 
   RowBox[{"Frame", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.667310863642853*^9, 3.6673109290079727`*^9}}],

