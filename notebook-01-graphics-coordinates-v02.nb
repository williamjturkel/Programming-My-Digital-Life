Cell[CellGroupData[{

Cell["Circle and Disk", "Subsection",
 CellChangeTimes->{{3.658338118278393*^9, 3.658338120149979*^9}}],

Cell[TextData[{
 "Here is how you use the ",
 StyleBox["Graphics",
  FontWeight->"Bold"],
 " command to draw a circle. You have to put the empty square brackets after \
the ",
 StyleBox["Circle",
  FontWeight->"Bold"],
 " command."
}], "Text",
 CellChangeTimes->{{3.658600335288493*^9, 3.6586003443110027`*^9}, {
  3.660150621198207*^9, 3.6601506336597633`*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"Circle", "[", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.658337743712842*^9, 3.658337755216345*^9}}],

Cell[TextData[{
 "A ",
 StyleBox["Disk",
  FontWeight->"Bold"],
 " is a filled circle. Here is how you draw one. You have to put the empty \
square brackets after the ",
 StyleBox["Disk",
  FontWeight->"Bold"],
 " command."
}], "Text",
 CellChangeTimes->{{3.658600352776116*^9, 3.658600364806333*^9}, {
  3.660150640828046*^9, 3.6601506540035467`*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"Disk", "[", "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.658337762766925*^9, 3.65833779667904*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Line weights, colours and styles", "Subsection",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.65833781547731*^9, 3.65833782049296*^9}, {3.6583380474598837`*^9, 
  3.658338056625495*^9}}],

Cell[TextData[{
 "Multiple drawing commands are enclosed in curly braces. A collection of \
items in curly braces is called a ",
 StyleBox["List",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.65833789467406*^9, 3.658337927191765*^9}, {
  3.658337981013967*^9, 3.65833799520446*^9}, {3.6583381647019863`*^9, 
  3.658338182475211*^9}}],

Cell[TextData[{
 StyleBox["Thick",
  FontWeight->"Bold"],
 " makes a wider line. Note that you have to change line thickness ",
 StyleBox["before",
  FontSlant->"Italic"],
 " you draw the circle."
}], "Text",
 CellChangeTimes->{{3.65833789467406*^9, 3.658337927191765*^9}, {
   3.658337981013967*^9, 3.65833799520446*^9}, 3.6583381647019863`*^9, {
   3.658341214094159*^9, 3.658341214382188*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"{", 
   RowBox[{"Thick", ",", 
    RowBox[{"Circle", "[", "]"}]}], "}"}], "]"}]], "Input",
 CellChangeTimes->{{3.658337831451344*^9, 3.6583378785702353`*^9}}],

Cell["\<\
This is how you change the line colour and style of the circle.\
\>", "Text",
 CellChangeTimes->{{3.658337949310628*^9, 3.6583379791347733`*^9}, {
  3.6583380128675537`*^9, 3.658338017147345*^9}, {3.6601506652755613`*^9, 
  3.660150668979618*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"{", 
   RowBox[{"Red", ",", "Dashed", ",", " ", 
    RowBox[{"Circle", "[", "]"}]}], "}"}], "]"}]], "Input",
 CellChangeTimes->{{3.658337937638762*^9, 3.658337945638957*^9}, {
  3.658338020484126*^9, 3.658338029742152*^9}}],

Cell[TextData[{
 "It doesn\[CloseCurlyQuote]t matter if you put ",
 StyleBox["Red",
  FontWeight->"Bold"],
 " before ",
 StyleBox["Dashed",
  FontWeight->"Bold"],
 " or vice versa, as long as both come before ",
 StyleBox["Circle",
  FontWeight->"Bold"],
 "."
}], "Text",
 CellChangeTimes->{{3.658597894946147*^9, 3.6585979117445*^9}}]
}, Open  ]]
}, Open  ]]
}, Open  ]],

