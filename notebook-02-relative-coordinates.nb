Cell["\<\
Last class we learned how to build up a graphic image, and to give a \
collection of commands a name.\
\>", "Text",
 CellChangeTimes->{{3.658599335954611*^9, 3.658599338961194*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"ufo", "=", 
   RowBox[{"{", 
    RowBox[{"Orange", ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"3", ",", "1"}], "}"}]}], "]"}], ",", "LightOrange", ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"1", "/", "2"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"3", "/", "2"}], ",", "1"}], "}"}]}], "]"}]}], "}"}]}], 
  ";"}]], "Input",
