Cell["A routine flight", "Subsection",
 CellChangeTimes->{{3.662205636768157*^9, 3.662205649093918*^9}}],

Cell[TextData[{
 "One of the things we can do with ",
 StyleBox["Manipulate",
  FontWeight->"Bold"],
 " is use it to create simple animations."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.662203328136526*^9, 3.6622033540256453`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawPlane", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"Darker", "[", "Gray", "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"10", ",", "1"}], "}"}]}], "]"}], ",", 
    RowBox[{"Rotate", "[", 
     RowBox[{
      RowBox[{"Disk", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "-", "9"}], ",", 
          RowBox[{"y", "+", 
           RowBox[{"3", "/", "2"}]}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", 
          RowBox[{"1", "/", "2"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"100", " ", "Degree"}]}], "]"}], ",", 
    RowBox[{"Rotate", "[", 
     RowBox[{
      RowBox[{"Disk", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "y"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"5", ",", "1"}], "}"}]}], "]"}], ",", 
      RowBox[{"135", "Degree"}]}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.662203376893289*^9, 3.662203756892147*^9}, {
  3.662203874186697*^9, 3.662203877720179*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"drawPlane", "[", 
    RowBox[{"{", 
     RowBox[{"0", ",", "0"}], "}"}], "]"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}], ",", 
