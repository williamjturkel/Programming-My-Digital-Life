  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"drawBuilding", "[", 
     RowBox[{"{", 
      RowBox[{"x", ",", "0"}], "}"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", "0", ",", "95", ",", "5"}], "}"}]}], "]"}]}]], "Input",\

 CellChangeTimes->{{3.6602157897634277`*^9, 3.6602158837628403`*^9}}],

