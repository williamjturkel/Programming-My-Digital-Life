       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "+", "4"}], ",", 
          RowBox[{"y", "+", "28"}]}], "}"}], ",", "r"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"r", ",", "1", ",", "7"}], "}"}]}], "]"}], ",", "Gray", ",", 
    "Thick", ",", 
    RowBox[{"Dashing", "[", "None", "]"}], ",", 
    RowBox[{"Line", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "24"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "8"}], ",", "y"}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Line", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "24"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "28"}]}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "+", "4"}], ",", 
        RowBox[{"y", "+", "28"}]}], "}"}], ",", 
      RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.660564463483501*^9, 3.660564594508457*^9}, {
  3.660564641866733*^9, 3.660564642354286*^9}}],

