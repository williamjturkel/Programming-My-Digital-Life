Cell[CellGroupData[{

Cell["Example: Radio Tower", "Section",
 CellChangeTimes->{{3.647716384690867*^9, 3.6477163853144817`*^9}, {
  3.6605638545119743`*^9, 3.660563860661599*^9}}],

Cell["\<\
Suppose we want to draw a radio tower. We can start by drawing a few lines to \
get the basic shape, and put a little ball on top.\
\>", "Text",
 CellChangeTimes->{{3.660563864645525*^9, 3.660563961840918*^9}, {
   3.660564067917543*^9, 3.660564073524085*^9}, 3.692468896842205*^9}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Gray", ",", "Thick", ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"8", ",", "0"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "28"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"4", ",", "28"}], "}"}], ",", 
       RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.660563947598452*^9, 3.660564123697859*^9}}],

Cell[TextData[{
 "We can add some radio waves by drawing concentric circles of thin, \
dot-dashed lines. (When we are done with the dashing we turn it off with ",
 StyleBox["Dashing[None]",
  FontWeight->"Bold"],
 "). These circles will all have the same centre point and differing radii."
}], "Text",
 CellChangeTimes->{{3.660564132666353*^9, 3.660564169911489*^9}, {
  3.660564261172913*^9, 3.6605642650673723`*^9}, {3.660564296788762*^9, 
  3.660564297641777*^9}, {3.660564363288064*^9, 3.6605643888534193`*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Blue", ",", "DotDashed", ",", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"Circle", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"4", ",", "28"}], "}"}], ",", "r"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"r", ",", "1", ",", "7"}], "}"}]}], "]"}], ",", "Gray", ",", 
     "Thick", ",", 
     RowBox[{"Dashing", "[", "None", "]"}], ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"8", ",", "0"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "28"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"4", ",", "28"}], "}"}], ",", 
       RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.660563947598452*^9, 3.660564123697859*^9}, {
  3.660564182209017*^9, 3.660564303705537*^9}, {3.66056435231358*^9, 
  3.660564357343441*^9}, {3.660564635547038*^9, 3.6605646361946774`*^9}}],

Cell["\<\
We can turn our commands into a drawing function with relative coordinates.\
\>", "Text",
 CellChangeTimes->{{3.660564434219327*^9, 3.6605644520424023`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"drawRadioTower", "[", 
   RowBox[{"{", 
    RowBox[{"x_", ",", "y_"}], "}"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"{", 
   RowBox[{"Blue", ",", "DotDashed", ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Circle", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"x", "+", "4"}], ",", 
          RowBox[{"y", "+", "28"}]}], "}"}], ",", "r"}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"r", ",", "1", ",", "7"}], "}"}]}], "]"}], ",", "Gray", ",", 
    "Thick", ",", 
    RowBox[{"Dashing", "[", "None", "]"}], ",", 
    RowBox[{"Line", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "24"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "8"}], ",", "y"}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Line", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "24"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"x", "+", "4"}], ",", 
         RowBox[{"y", "+", "28"}]}], "}"}]}], "}"}], "]"}], ",", 
    RowBox[{"Disk", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"x", "+", "4"}], ",", 
        RowBox[{"y", "+", "28"}]}], "}"}], ",", 
      RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}]}]], "Input",
 CellChangeTimes->{{3.660564463483501*^9, 3.660564594508457*^9}, {
  3.660564641866733*^9, 3.660564642354286*^9}}],

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{"drawRadioTower", "[", 
   RowBox[{"{", 
    RowBox[{"0", ",", "0"}], "}"}], "]"}], "]"}]], "Input",
 CellChangeTimes->{{3.660564479957344*^9, 3.660564488936808*^9}, {
  3.660564599396501*^9, 3.660564600708005*^9}}]
}, Open  ]]
