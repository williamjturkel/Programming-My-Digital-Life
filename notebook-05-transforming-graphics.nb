Cell["\<\
Suppose we want to draw something like a radio tower. We can start by drawing \
a few lines to get the basic shape, and put a little ball on top.\
\>", "Text",
 CellChangeTimes->{{3.660563864645525*^9, 3.660563961840918*^9}, {
  3.660564067917543*^9, 3.660564073524085*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Graphics", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Gray", ",", "Thick", ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "0"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"8", ",", "0"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Line", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"4", ",", "24"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"4", ",", "28"}], "}"}]}], "}"}], "]"}], ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"4", ",", "28"}], "}"}], ",", 
       RowBox[{"1", "/", "2"}]}], "]"}]}], "}"}], ",", 
   RowBox[{"Frame", "\[Rule]", "True"}], ",", 
   RowBox[{"GridLines", "\[Rule]", "Automatic"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.660563947598452*^9, 3.660564123697859*^9}}],

Cell[BoxData[
 GraphicsBox[
  {GrayLevel[0.5], Thickness[Large], LineBox[{{0, 0}, {4, 24}, {8, 0}}], 
