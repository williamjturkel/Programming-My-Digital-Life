Cell[TextData[{
 "The ",
 StyleBox["StringReplace",
  FontWeight->"Bold"],
 " command takes a string and a list of replacements. You can use it to make \
a simple cipher. Here we replace only a single character."
}], "Text",
 CellChangeTimes->{{3.6477163758185587`*^9, 3.647716376530898*^9}, {
  3.6661131693727713`*^9, 3.6661131959720182`*^9}, {3.6661133312394247`*^9, 
  3.6661133668454733`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"StringReplace", "[", 
  RowBox[{"\"\<This is a message to be encrypted\>\"", ",", 
   RowBox[{"{", 
    RowBox[{"\"\<a\>\"", "\[Rule]", "\"\<c\>\""}], "}"}], ",", 
   RowBox[{"IgnoreCase", "\[Rule]", "True"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.666113063082747*^9, 3.6661131182316027`*^9}, {
   3.66611315147103*^9, 3.6661131603332367`*^9}, {3.6661132508267527`*^9, 
   3.6661132565218573`*^9}, 3.666113321472283*^9}],

Cell[BoxData["\<\"This is c messcge to be encrypted\"\>"], "Output",
 CellChangeTimes->{
  3.666113119433339*^9, {3.666113152497025*^9, 3.666113161227903*^9}, 
   3.666113256932385*^9, 3.666113324079153*^9}]
}, Open  ]],

Cell[TextData[{
 "We could type in our whole list of rules for encryption, but we will build \
the list using some list commands and ",
 StyleBox["MapThread",
  FontWeight->"Bold"],
 " instead."
}], "Text",
