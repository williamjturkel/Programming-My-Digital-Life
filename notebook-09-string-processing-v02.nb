}, Open  ]],

Cell[TextData[{
 "The ",
 StyleBox["StringJoin",
  FontWeight->"Bold"],
 " command takes a list of strings and joins them to create a single string. \
Note that blank spaces are not automatically included."
}], "Text",
 CellChangeTimes->{{3.6661119038934402`*^9, 3.666111950371765*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"StringJoin", "[", 
  RowBox[{"{", 
   RowBox[{
   "\"\<what\>\"", ",", "\"\<kind\>\"", ",", "\"\<of\>\"", ",", 
    "\"\<thing\>\""}], "}"}], "]"}]], "Input",
 CellChangeTimes->{{3.6661119202163343`*^9, 3.666111937452183*^9}}],

Cell[BoxData["\<\"whatkindofthing\"\>"], "Output",
 CellChangeTimes->{3.666111938095195*^9}]
}, Open  ]],

Cell[TextData[{
 "The ",
 StyleBox["StringRiffle",
  FontWeight->"Bold"],
 " command joins a list of strings together separated by a separator. Here a \
blank space is used."
}], "Text",
 CellChangeTimes->{{3.6661119688509007`*^9, 3.6661119895062304`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"StringRiffle", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
    "\"\<what\>\"", ",", "\"\<kind\>\"", ",", "\"\<of\>\"", ",", 
     "\"\<thing\>\""}], "}"}], ",", "\"\< \>\""}], "]"}]], "Input",
 CellChangeTimes->{{3.666111952841277*^9, 3.666111964659151*^9}}],

Cell[BoxData["\<\"what kind of thing\"\>"], "Output",
 CellChangeTimes->{3.666111965418035*^9}]
}, Open  ]],

Cell[TextData[{
 "The ",
 StyleBox["StringSplit",
  FontWeight->"Bold"],
 " command splits a string into a list of strings, using whitespace as the \
default separator."
}], "Text",
 CellChangeTimes->{{3.666112060928117*^9, 3.666112107629671*^9}}],

