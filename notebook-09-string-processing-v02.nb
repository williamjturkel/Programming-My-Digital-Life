Cell[TextData[{
 "A string is a sequence of characters (possibly including ",
 StyleBox["whitespace",
  FontSlant->"Italic"],
 " like spaces, tabs, etc.) enclosed in a pair of quotation marks. You can \
find out what something is in ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " with the Head command."
}], "Text",
 CellChangeTimes->{{3.666111655870875*^9, 3.666111704277033*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Head", "[", "\"\<What kind of thing is this?\>\"", "]"}]], "Input",
 CellChangeTimes->{{3.6661117072367887`*^9, 3.6661117150848503`*^9}}],

Cell[BoxData["String"], "Output",
 CellChangeTimes->{3.6661117155043087`*^9}]
}, Open  ]],

Cell[TextData[{
 "The ",
 StyleBox["StringLength",
  FontWeight->"Bold"],
 " command counts the number of characters in a string."
}], "Text",
 CellChangeTimes->{{3.666111720396327*^9, 3.66611172966022*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"StringLength", "[", "\"\<What kind of thing is this?\>\"", 
  "]"}]], "Input",
 CellChangeTimes->{{3.666111731436277*^9, 3.666111738815668*^9}}],

Cell[BoxData["27"], "Output",
 CellChangeTimes->{3.666111739771626*^9}]
}, Open  ]],

